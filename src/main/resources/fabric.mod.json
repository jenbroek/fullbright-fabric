{
	"schemaVersion": 1,
	"id": "${modId}",
	"version": "${modVersion}",
	"name": "${modName}",
	"description": "${modDescription}",

	"authors": [
		"jenbroek"
	],
	"contact": {
		"sources": "https://github.com/jenbroek/${modId}-fabric"
	},
	"license": "BSD-3-Clause",

	"environment": "client",
	"mixins": [
		"${modId}.mixins.json"
	],
	"entrypoints": {
		"client": [
			"com.github.jenbroek.fullbright.FullbrightClientInitializer"
		]
	},
	"depends": {
		"fabricloader": ">=${fabricLoaderVersion}",
		"fabric": "^${fabricApiVersion}",
		"minecraft": "~${minecraftVersion}",
		"java": ">=${javaVersion}"
	}
}
